<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:view="using:KyoshinEewViewer.Views"
        xmlns:vm="using:KyoshinEewViewer.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		MinHeight="400" MinWidth="600" WindowStartupLocation="CenterScreen"
        x:Class="KyoshinEewViewer.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel" x:CompileBindings="True"
		Icon="/Assets/logo.ico"
        Title="{Binding Title}" Bounds="{Binding Bounds, Mode=OneWayToSource}">
	<Design.DataContext>
		<vm:MainWindowViewModel />
	</Design.DataContext>
	<view:MainView Name="MainView" />
</Window>
